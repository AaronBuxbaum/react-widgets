  "scripts": {
    "test": "npm run lint && npm run test-only",
    "test-only": "jest --runInBand",
    "tdd": "jest --watch --runInBand",
    "lint": "eslint src test",
    "scss": "node-sass src/scss/react-widgets.scss -o dist/css",
    "styles": "cpy src/scss/* lib/scss/",
    "assets:lib": "cpy src/img/* lib/img/",
    "assets:dist": "cpy src/img/* dist/img/",
    "compile:lib": "build src",
    "compile:dist": "rimraf ./dist/* cross-env NODE_ENV=production webpack",
    "build:lib": "npm run compile:lib && npm run styles && npm run assets:lib",
    "build:dist": "npm run compile:dist && npm run scss && npm run assets:dist",
    "build": "npm run build:lib && npm run build:dist",
    "prepublishOnly": "npm run build"
  },
  "homepage": "http://jquense.github.io/react-widgets/docs/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jquense/react-widgets"
